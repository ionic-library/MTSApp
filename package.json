{
  "name": "MTSApp",
  "version": "0.0.1",
  "author": "Ionic Framework",
  "homepage": "http://ionicframework.com/",
  "private": true,
  "config": {
    "ionic_copy": "./scripts/copy-custom-libs.js"
  },
  "scripts": {
    "clean": "ionic-app-scripts clean",
    "build": "ionic-app-scripts build",
    "lint": "ionic-app-scripts lint",
    "ionic:build": "ionic-app-scripts build",
    "ionic:serve": "ionic-app-scripts serve",
    "test": "karma start ./test-config/karma.conf.js --browsers Chrome",
    "test-ci": "karma start ./test-config/karma.conf.js --single-run --browsers ChromeHeadless",
    "test-coverage": "karma start ./test-config/karma.conf.js --coverage --browsers ChromeHeadless",
    "test-headless": "karma start ./test-config/karma.conf.js --browsers ChromeHeadless",
    "e2e": "npm run e2e-update && npm run e2e-test",
    "e2e-test": "protractor ./test-config/protractor.conf.js",
    "e2e-update": "webdriver-manager update --standalone false --gecko false",
    "prepush": "karma start ./test-config/karma.conf.js --single-run --browsers ChromeHeadless",
    "precommit": "pretty-quick --staged && tslint -c ./tslint.json --project ./tsconfig.json --format stylish"
  },
  "dependencies": {
    "@angular/animations": "5.2.10",
    "@angular/common": "5.2.10",
    "@angular/compiler": "5.2.10",
    "@angular/compiler-cli": "5.2.10",
    "@angular/core": "5.2.10",
    "@angular/forms": "5.2.10",
    "@angular/http": "5.2.10",
    "@angular/platform-browser": "5.2.10",
    "@angular/platform-browser-dynamic": "5.2.10",
    "@ionic-native/camera": "4.3.3",
    "@ionic-native/core": "4.7.0",
    "@ionic-native/splash-screen": "4.7.0",
    "@ionic-native/status-bar": "4.7.0",
    "@ionic-native/unique-device-id": "^4.9.1",
    "@ionic/pro": "1.0.20",
    "@ionic/storage": "2.1.3",
    "@ngx-translate/core": "8.0.0",
    "@ngx-translate/http-loader": "^2.0.0",
    "autodll-webpack-plugin": "^0.3.9",
    "cordova-plugin-uniquedeviceid": "^1.3.2",
    "font-awesome": "^4.7.0",
    "hard-source-webpack-plugin": "^0.7.6",
    "ionic-angular": "3.9.2",
    "ionicons": "3.0.0",
    "lodash": "^4.17.10",
    "luhn": "^2.3.0",
    "rxjs": "5.5.10",
    "sw-toolbox": "3.6.0",
    "tslint": "^5.10.0",
    "winston": "^3.0.0",
    "zone.js": "0.8.26"
  },
  "devDependencies": {
    "@angular/cli": "1.4.8",
